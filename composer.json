{
    "name": "manuxi/sulu-event-bundle",
    "description": "Say hello to sulu-events",
    "type": "sulu-bundle",
    "keywords": [
        "sulu",
        "suluCms",
        "event"
    ],
    "homepage": "https://github.com/manuxi/SuluEventBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Manuel Bertrams",
            "email": "manu@bytefresser.de"
        }
    ],
    "require": {
        "dantleech/phpcr-migrations-bundle": "^1.2",
        "php": "^7.4 | ^8.0",
        "sulu/sulu": "^2.4.1",
        "symfony/config": "^4.4 | ^5.0 | ^6.0",
        "symfony/dependency-injection": "^4.4 | ^5.0 | ^6.0",
        "symfony/framework-bundle": "^4.4 | ^5.0 | ^6.0",
        "symfony/http-foundation": "^4.4 | ^5.0 | ^6.0",
        "symfony/http-kernel": "^4.4 | ^5.0 | ^6.0",
        "symfony/intl": "^5.2 | ^6.0",
        "symfony/translation": "^5.2 | ^6.0"
    },
    "require-dev": {
        "jackalope/jackalope-doctrine-dbal": "^1.3.4",
        "phpunit/phpunit": "^8.0",
        "symfony/browser-kit": "^4.4 | ^5.0 | ^6.0",
        "symfony/phpunit-bridge": "^4.4 | ^5.0 | ^6.0",
        "symfony/console": "^4.4 | ^5.0 | ^6.0",
        "phpspec/prophecy": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "Manuxi\\SuluEventBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Manuxi\\SuluEventBundle\\Tests\\": "Tests"
        }
    },
    "scripts": {
        "test": [
            "@phpunit"
        ],
        "phpunit": "vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
